    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"import torch\\n\",\n",
    "    \"\\n\",\n",
    "    \"def sample_negatives_per_user_bipartite(edge_index,\\n\",\n",
    "    \"                                        num_users,\\n\",\n",
    "    \"                                        num_items,\\n\",\n",
    "    \"                                        num_neg_per_user=10,\\n\",\n",
    "    \"                                        only_users_with_edges=True,\\n\",\n",
    "    \"                                        device=None):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Sample negative userâ€“item edges on a bipartite graph:\\n\",\n",
    "    \"    - For each user u, sample `num_neg_per_user` items i that are NOT connected to u.\\n\",\n",
    "    \"    - Returns neg_edge_index: [2, num_negatives_total]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    Args:\\n\",\n",
    "    \"        edge_index: LongTensor [2, E], with user->item edges\\n\",\n",
    "    \"        num_users: int, number of user nodes (0..num_users-1)\\n\",\n",
    "    \"        num_items: int, number of item nodes (num_users..num_users+num_items-1)\\n\",\n",
    "    \"        num_neg_per_user: int, #negatives per user\\n\",\n",
    "    \"        only_users_with_edges: if True, only sample negatives for users that have at least one positive edge\\n\",\n",
    "    \"        device: torch.device or None (defaults to edge_index.device)\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    if device is None:\\n\",\n",
    "    \"        device = edge_index.device\\n\",\n",
    "    \"\\n\",\n",
    "    \"    users = edge_index[0].tolist()\\n\",\n",
    "    \"    items = edge_index[1].tolist()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Build adjacency: for each user, set of *item indices in global space*\\n\",\n",
    "    \"    pos_items_per_user = {u: set() for u in range(num_users)}\\n\",\n",
    "    \"    for u, i in zip(users, items):\\n\",\n",
    "    \"        pos_items_per_user[u].add(i)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    neg_user_list = []\\n\",\n",
    "    \"    neg_item_list = []\\n\",\n",
    "    \"\\n\",\n",
    "    \"    for u in range(num_users):\\n\",\n",
    "    \"        # skip users with no positives if desired\\n\",\n",
    "    \"        if only_users_with_edges and len(pos_items_per_user[u]) == 0:\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # We'll sample until we have `num_neg_per_user` unique negatives for this user\\n\",\n",
    "    \"        user_neg_items = set()\\n\",\n",
    "    \"        max_trials = num_neg_per_user * 10  # safety to avoid infinite loops in dense graphs\\n\",\n",
    "    \"        trials = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"        while len(user_neg_items) < num_neg_per_user and trials < max_trials:\\n\",\n",
    "    \"            # sample item index in [0, num_items), then shift to global item ids\\n\",\n",
    "    \"            i_local = torch.randint(0, num_items, (1,), device=device).item()\\n\",\n",
    "    \"            i_global = i_local + num_users\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # Must not be a positive edge and not already sampled as negative for this user\\n\",\n",
    "    \"            if (i_global not in pos_items_per_user[u]) and (i_global not in user_neg_items):\\n\",\n",
    "    \"                user_neg_items.add(i_global)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            trials += 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # if graph is very dense some users may get fewer than num_neg_per_user negatives\\n\",\n",
    "    \"        for i_global in user_neg_items:\\n\",\n",
    "    \"            neg_user_list.append(u)\\n\",\n",
    "    \"            neg_item_list.append(i_global)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    neg_users = torch.tensor(neg_user_list, device=device, dtype=torch.long)\\n\",\n",
    "    \"    neg_items = torch.tensor(neg_item_list, device=device, dtype=torch.long)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    neg_edge_index = torch.stack([neg_users, neg_items], dim=0)\\n\",\n",
    "    \"    return neg_edge_index\\n\"\n",
    "   ],\n",
